{
  "name": "trilogy-studio",
  "private": true,
  "version": "0.0.3",
  "description": "A desktop IDE for running PreQL and SQL queries and building PreQL models.",
  "author": {
    "name": "Trilogy/PreQL Team",
    "email": "ethan.dickinson@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/preqldata/preql-studio.git"
  },
  "scripts": {
    "dev": "vite",
    "build_debug": "vue-tsc && vite build",
    "build": "node build-backend.js && vue-tsc && vite build",
    "preview": "vite preview",
    "debug_bg": "node test-bg.js",
    "dist_linux": "vue-tsc && vite build && node build-backend.js && electron-builder --linux snap",
    "dist_windows": "vue-tsc && vite build && node build-backend.js && electron-builder --win",
    "test": "playwright test"
  },
  "dependencies": {
    "axios": "^1.4.0",
    "dotenv": "^16.3.1",
    "electron-store": "^8.1.0",
    "monaco-editor": "^0.40.0",
    "noty": "^3.2.0-beta-deprecated",
    "split.js": "^1.6.5",
    "tabulator-tables": "^5.5.0",
    "vite-plugin-commonjs-externals": "^0.1.3",
    "vite-plugin-vuetify": "^1.0.2",
    "vue": "^3.2.47",
    "vue-router": "^4.2.4",
    "vue3-apexcharts": "^1.4.1",
    "vue3-shortkey": "^4.0.0",
    "vuetify": "^3.3.7",
    "vuex": "^4.1.0"
  },
  "devDependencies": {
    "@mdi/font": "^7.2.96",
    "@playwright/test": "^1.37.1",
    "@vitejs/plugin-vue": "^4.1.0",
    "electron": "^26.1.0",
    "electron-builder": "^24.6.4",
    "sass": "^1.66.1",
    "sass-loader": "^13.3.2",
    "typescript": "^5.0.2",
    "vite": "^4.3.2",
    "vite-plugin-electron": "^0.11.2",
    "vite-plugin-electron-renderer": "^0.14.5",
    "vue-tsc": "^1.4.2"
  },
  "main": "dist-electron/main.js"
}
